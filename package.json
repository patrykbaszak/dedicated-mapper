{
    "version": "1.1.0",
    "dependencies": {
        "standard-version": "*"
    },
    "scripts": {
        "version:patch": "standard-version",
        "version:minor": "standard-version --release-as minor",
        "version:major": "standard-version --release-as major",
        "version:release": "git push --follow-tags origin develop",
        "release": "version:release"
    }
}
